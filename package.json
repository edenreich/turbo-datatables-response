    "@types/node": "^10.10.3",
    "koa": "^2.5.3",
    "koa-router": "^7.4.0",
    "mysql": "^2.16.0",
    "nodemon": "^1.18.4",
    "ts-loader": "^5.1.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  }
